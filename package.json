{
  "name": "chomsky-ng2",
  "version": "1.2.1",
  "description": "An Angular 2 module that leverages Chomsky for i18n.",
  "scripts": {
    "clean": "./node_modules/.bin/del bundles coverage lib dist",
    "compile": "npm run compile:lib && npm run compile:system",
    "compile:lib": "babel src --ignore \".spec.js, testing/**\" --out-dir lib --copy-files",
    "compile:system": "./config/bundle-system.js",
    "prepublish": "npm run clean && npm run compile",
    "postpublish": "npm run deploy:gh-pages",
    "lint": "eslint src demo",
    "lint:save": "eslint src demo -f node_modules/static-code-analysis/formatters/eslint.js > ./coverage/eslint.json",
    "loc": "sloc src/ --format json > ./coverage/sloc.json",
    "start": "webpack-dev-server --hot --inline --colors --display-error-details --display-cached",
    "test": "NODE_ENV=test ./node_modules/.bin/karma start",
    "test:watch": "NODE_ENV=test ./node_modules/.bin/karma start --single-run false --auto-watch true",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular -v",
    "github-release": "conventional-github-releaser -p angular",
    "deploy:gh-pages": "npm run build:prod && ./node_modules/.bin/gh-pages -d dist",
    "preversion": "npm test",
    "version": "npm run changelog && git add -A",
    "postversion": "git push origin master && git push --tags && npm run github-release && npm publish",
    "build:prod": "webpack --config config/webpack.prod.js  --progress --profile --colors --display-error-details --display-cached --bail",
    "presca": "npm run lint:save && npm run loc",
    "sca": "sca"
  },
  "main": "lib/chomsky-ng2.js",
  "keywords": [
    "angular2",
    "translations"
  ],
  "author": "Kameron R. Sween <kameron@krsween.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bullhorn/chomsky-ng2.git"
  },
  "bugs": {
    "url": "https://github.com/bullhorn/chomsky-ng2/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/bullhorn/chomsky-ng2",
  "dependencies": {
    "chomsky": "~1.1.0"
  },
  "devDependencies": {
    "@angular/http": "2.0.0-rc.5",
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/platform-server": "2.0.0-rc.5",
    "angular2-hmr": "^0.7.0",
    "async": "^2.0.0-rc.3",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "~6.14.0",
    "babel-core": "~6.14.0",
    "babel-eslint": "~6.1.2",
    "babel-istanbul-loader": "^0.1.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-__coverage__": "^0.111111.1",
    "babel-plugin-angular2-annotations": "~5.1.0",
    "babel-plugin-transform-class-properties": "~6.8.0",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-plugin-transform-flow-strip-types": "~6.8.0",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "~7.3.0",
    "babylon": "6.8.0",
    "compression-webpack-plugin": "0.3.1",
    "conventional-changelog": "1.1.0",
    "conventional-changelog-cli": "1.1.1",
    "conventional-github-releaser": "1.1.2",
    "copy-webpack-plugin": "2.1.3",
    "css-loader": "^0.23.1",
    "del": "2.2.0",
    "del-cli": "0.2.0",
    "eslint": "2.9.0",
    "es6-promise": "3.2.1",
    "es6-shim": "0.35.1",
    "es7-reflect-metadata": "1.6.0",
    "eslint-config-bullhorn": "~0.1.0",
    "eslint-loader": "^1.3.0",
    "gh-pages": "0.11.0",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.16.1",
    "jasmine": "2.4.1",
    "karma": "0.13.22",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.0.0",
    "karma-jasmine": "1.0.2",
    "karma-phantomjs-launcher": "1.0.0",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "1.7.0",
    "moment": "^2.13.0",
    "node-sass": "^3.4.2",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "2.1.7",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.2",
    "require-dir": "0.3.0",
    "rxjs": "5.0.0-beta.6",
    "sass-loader": "^3.2.0",
    "sloc": "^0.1.10",
    "source-map-loader": "0.1.5",
    "static-code-analysis": "0.0.9",
    "style-loader": "^0.13.1",
    "systemjs-builder": "0.15.16",
    "typescript": "^1.8.10",
    "underscore": "^1.8.3",
    "webpack": "1.13.0",
    "webpack-dev-server": "1.14.1",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^0.12.0",
    "zone.js": "^0.6.14"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint"
    }
  },
  "staticCodeAnalysis": {
    "type": "javascript",
    "public": true,
    "locations": {
      "eslint": "/coverage/eslint.json",
      "sloc": "/coverage/sloc.json",
      "karma-coverage": "/coverage/coverage.txt"
    }
  }
}
