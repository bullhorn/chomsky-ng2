{
  "name": "chomsky-ng2",
  "version": "0.0.1",
  "description": "An Angular 2 module that leverages Chomsky for i18n.",
  "scripts": {
    "postinstall": "webdriver-manager update",
    "start": "npm run serve",
    "build": "gulp",
    "watch": "gulp watch",
    "clean": "gulp clean",
    "test": "npm run unit",
    "unit": "karma start --single-run",
    "tdd": "karma start",
    "serve": "concurrently \"npm run build\" \"http-server public\"",
    "compile:watch": "babel src --ignore .spec.js --out-dir lib --watch",
    "prepublish": "npm run compile"
  },
  "main": "src/library.js",
  "author": "Kameron R. Sween <kameron@krsween.com>",
  "repository": "krsween/chomsky-ng2",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.6.4",
    "babel-eslint": "^6.0.2",
    "babel-plugin-angular2-annotations": "^5.0.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.3",
    "babel-plugin-transform-flow-strip-types": "^6.6.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "babylon": "6.6.4",
    "browserify": "^13.0.0",
    "concurrently": "^2.0.0",
    "del": "^2.2.0",
    "eslint": "^2.6.0",
    "gulp": "^3.9.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.15",
    "karma-browserify": "^5.0.2",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-source-map-support": "^1.1.0",
    "karma-spec-reporter": "0.0.25",
    "object-assign": "^4.0.1",
    "phantomjs-prebuilt": "^2.1.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0",
    "webpack-stream": "^3.1.0"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.13",
    "chomsky": "^0.1.2",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "^0.6.6"
  }
}
